package lesson;

import java.util.LinkedList;

public class Main {
	
	
	public static void main(String[] args) {
		 
		LinkedList<String> name = new LinkedList<>();
		name.add("Ranshika");
		name.add("Perera");
		name.add("Amara");
		name.add("Salmal");
		name.addFirst("Abeykoon");
		name.addLast("Mihiran");
		name.removeLast();
		
		
		//name.clear();
		//name.add(1, "index1");
		
		for (String x : name) {
			System.out.println(x);
		}
		
		System.out.println(name.size());
	   
	
	}


}
	


