package Tutorial;

public class hello {
	
	/*
	 * byte (number , 1 byte)
	 * short (number , 2 byte)
	 * int (number, 4 byte)
	 * long (number, 8 byte)
	 * Float (float number , 4 byte)
	 * double (float number , 8 byte)
	 * char(a character, 2 byte)
	 * boolean (true or fales , 1 byte)
	 * 
	 */

	public static void main(String[] args) {
		
		int my_variable = 10;
		float my_decimal = (float) 4.5;
		double my_double = 10.5;
		char my_char = 'A';
		boolean is_true = false;
		
		
		
	    System.out.println(my_variable);
	    System.out.println(my_decimal);
	    System.out.println(my_double);
	    System.out.println(my_char);
      System.out.println(is_true);
        


	}

}
