package Tutorials;

public class Myclass {
	
	

	public static void main(String[] args) {
		
		int myArrray [] = {1,2,3,4,5};
		int [] myArray2 = {1,2,3,4,5};
		
		
		System.out.println(myArray2[1]);
		System.out.println("--------------");
		
		int index = 0;
		while(index<5) {
			System.out.print(myArrray[index]);
			index++;
		}
		
	}

}
